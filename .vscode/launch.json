  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "raycast_proxy.main:app",
        "--host",
        "0.0.0.0",
        "--port",
        "443",
        "--ssl-certfile",
        "${workspaceFolder}/cert/backend.raycast.com.cert.pem",
        "--ssl-keyfile",
        "${workspaceFolder}/cert/backend.raycast.com.key.pem"
      ],
      "env": {
        "LOG_LEVEL": "DEBUG",
        "PYTHONPATH": "${workspaceFolder}/__pypackages__/3.9/lib",
        "DNS_SERVER": "1.1.1.1"
      },
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Attach to Docker",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "/project"
        }
      ],
      "justMyCode": true
    }
  ]
}
